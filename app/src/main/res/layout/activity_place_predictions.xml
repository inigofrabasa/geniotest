<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/rl_activity_places_view"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.inigoflores.geniotest.mapplaces.view.PlaceActivity">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.inigoflores.geniotest.mapplaces.view.MapsActivity" />

    <include layout="@layout/content_pick_location" />

</RelativeLayout>
